.nav.fixed{
  width:100%;
  position: fixed;
  left:0;
  top:0;
  z-index: 99999999;
}
.nav.hasBorder{
  border-bottom:1rpx solid #eee;
}
.navbar .backBtn{
  width:31rpx;
  height:31rpx;
}

