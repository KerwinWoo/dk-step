/**app.wxss**/
.clearfix:after{
  content:'';
  clear:both;
  display: block;
  height:0;
  visibility: hidden;
}
.col2-1{
  width:50%;
  float:left;
}
.container{
  width:100%;
  box-sizing: border-box;
  color:#333;
  font-size: 22rpx;
  font-family: "microsoft yahei", Arial;
}
.container.hastop{
  margin-top:128rpx;
}
.w{
  width:100%;
  box-sizing: border-box;
  padding:0 28rpx;
  background:#fff;
}
.defImgbg{
  background-image:url(data:image/jpeg;base64,)
}
.btnhover{
  opacity: .9;
}
.mask{
  position:fixed;
  width:100%;
  height:100%;
  background:rgba(0,0,0,0.5);
  left:0;
  top:0;
  z-index: 99999;
}

page::after{
  content: '';
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 2rpx;
  background-color: #eee;
  z-index: 9999;
}
.goodlist{
  background:#fff;
  padding:30rpx 30rpx 0 30rpx;
  box-sizing: border-box;
}
.goodlist .itm{
  width:50%;
  box-sizing: border-box;
  padding:0 15rpx;
  float:left;
  margin-bottom: 30rpx;
}
.goodlist .itm:nth-of-type(2n+1){
  padding-left:0;
}
.goodlist .itm:nth-of-type(2n){
  padding-right:0;
}
.goodlist image{
  width:100%;
  height:100%;
}
.goodlist .wrp{
  box-shadow:0px 0px 19rpx 0px rgba(89,89,89,0.17);
  border-radius:10rpx;
  width:100%;
  box-sizing: border-box;
  overflow: hidden;
}
.goodlist .wrp .photo{
  width:100%;
  height:0;
  padding-bottom: 100%;
  box-sizing: border-box;
  position: relative;
}
.goodlist .wrp .photo image-loader image{
  position: absolute;
  left:0;
  top:0;
}
.goodlist .wrp .content{
  padding:20rpx 12rpx;
  box-sizing: border-box;
  width:100%;
}
.goodlist .wrp .info{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  margin:6rpx 0 20rpx 0;
}
.goodlist .wrp .name{
  font-size:24rpx;
}
.goodlist .wrp .info image{
  width:26rpx;
  height:30rpx;
  margin-right:6rpx;
}
.goodlist .wrp .info .l{
  color:#FF8E00;
  font-weight: bold;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.goodlist .wrp .info .r{
  color:#999999;
  font-size: 16rpx;
}
.goodlist .wrp .exchangeBtn{
  width:200rpx;
  height:50rpx;
  line-height: 50rpx;
  text-align: center;
  color:#FFA110;
  font-size:2r4px;
  border:1px solid rgba(255,161,16,1);
  border-radius:10rpx;
  margin:0 auto;
}
.common_inviteBtn{
  position:fixed;
  width:450rpx;
  height:86rpx;
  background:linear-gradient(0deg,rgba(244,181,68,1) 0%,rgba(242,162,65,1) 100%);
  box-shadow:0px 1px 24rpx 0px rgba(255,107,14,0.21);
  border-radius:43rpx;
  text-align: center;
  line-height: 86rpx;
  left:50%;
  bottom:30rpx;
  margin-left:-225rpx;
  font-size:40rpx;
  font-weight:bold;
  color:#fff;
}