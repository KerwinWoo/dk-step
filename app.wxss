/**app.wxss**/
.clearfix:after{
  content:'';
  clear:both;
  display: block;
  height:0;
  visibility: hidden;
}
.col2-1{
  width:50%;
  float:left;
}
.container{
  width:100%;
  box-sizing: border-box;
  color:#333;
  font-size: 22rpx;
  font-family: "microsoft yahei", Arial;
}
.container.hastop{
  margin-top:128rpx;
}
.w{
  width:100%;
  box-sizing: border-box;
  padding:0 28rpx;
  background:#fff;
}
.defImgbg{
  background-image:url(data:image/jpeg;base64,)
}
.btnhover{
  opacity: .9;
}
.mask{
  position:fixed;
  width:100%;
  height:100%;
  background:rgba(0,0,0,0.5);
  left:0;
  top:0;
  z-index: 99999;
}
.layer{
  position: fixed;
  width:100%;
  height:100%;
  background:rgba(0,0,0,.23);
  padding:30rpx;
  left:0;
  top:0;
  z-index: 99999;
  box-sizing: border-box;
}
.flex-c{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.flex-side{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}
page::after{
  content: '';
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 2rpx;
  background-color: #eee;
  z-index: 9999;
}
.goodlist{
  background:#fff;
  padding:30rpx 30rpx 0 30rpx;
  box-sizing: border-box;
}
.goodlist .itm{
  width:50%;
  box-sizing: border-box;
  padding:0 15rpx;
  float:left;
  margin-bottom: 30rpx;
}
.goodlist .itm:nth-of-type(2n+1){
  padding-left:0;
}
.goodlist .itm:nth-of-type(2n){
  padding-right:0;
}
.goodlist image{
  width:100%;
  height:100%;
}
.goodlist .wrp{
  box-shadow:0px 0px 19rpx 0px rgba(89,89,89,0.17);
  border-radius:10rpx;
  width:100%;
  box-sizing: border-box;
  overflow: hidden;
}
.goodlist .wrp .photo{
  width:100%;
  height:0;
  padding-bottom: 100%;
  box-sizing: border-box;
  position: relative;
}
.goodlist .wrp .photo image-loader{
  position: absolute;
  left:0;
  top:0;
  width:100%;
  height:100%;
  overflow: hidden;
}
.goodlist .wrp .content{
  padding:20rpx 12rpx;
  box-sizing: border-box;
  width:100%;
}
.goodlist .wrp .info{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  margin:6rpx 0 20rpx 0;
}
.goodlist .wrp .name{
  font-size:24rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.goodlist .wrp .info image{
  width:32rpx;
  height:32rpx;
  margin-right:6rpx;
}
.goodlist .wrp .info .l{
  color:#FF8E00;
  font-weight: bold;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  height:32rpx;
  line-height: 32rpx;
  font-size:20rpx;
}
.plus{
  margin:0 4rpx;
}
.unit{
  font-weight: 400;
}
.goodlist .wrp .info .l text{
  padding-top:6rpx;
}
.goodlist .wrp .info .r{
  color:#999999;
  font-size: 16rpx;
}
.goodlist .wrp .exchangeBtn{
  width:200rpx;
  height:50rpx;
  line-height: 50rpx;
  text-align: center;
  color:#FB793B;
  font-size:24rpx;
  border:1px solid #FB793B;
  border-radius:10rpx;
  margin:0 auto;
}
.common_inviteBtn{
  position:fixed;
  width:450rpx;
  height:86rpx;
  background:linear-gradient(90deg,rgba(254,99,54,1) 0%,rgba(243,164,71,1) 100%);
  box-shadow:0px 1px 24rpx 0px rgba(255,107,14,0.21);
  border-radius:43rpx;
  text-align: center;
  line-height: 86rpx;
  left:50%;
  bottom:30rpx;
  margin-left:-225rpx;
  font-size:40rpx;
  font-weight:bold;
  color:#fff;
}
::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
}
.btn-default{
  background:linear-gradient(90deg,rgba(254,99,54,1) 0%,rgba(243,164,71,1) 100%)!important;
}
.btn-simple{
  border:1px solid #FE6134;
  color:#FB793B!important;
}
.ctr{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}
.dknum image{
  width:32rpx!important;
  height:32rpx!important;
  margin-right:8rpx;
}
.dknum{
  height:32rpx;
  display: flex;
  align-items: center;
  flex-direction: row;
  justify-content: flex-start;
}
.dknum text{
  display: inline-block;
  height:30rpx;
  line-height:30rpx;
  color:#FB793B;
  font-size: 24rpx;
}
.resetBtn{
  position:relative;
  display:inline-block;
  margin:0;
  padding:0;
  font-size:inherit;
  text-align:center;
  text-decoration:none;
  line-height:inherit;
  border-radius:0;
  -webkit-tap-highlight-color:transparent;
  overflow:inherit;
  color:none;
  background-color:transparent;
}
.resetBtn::after,.common_inviteBtn::after{
  border: none;
}
.closeBtn{
  width:60rpx;
  height:60rpx;
}
.commontoast{
  width:100%;
  height:100%;
  position: fixed;
  z-index: 9999999;
  box-sizing: border-box;
  overflow: hidden;
  background:rgba(0,0,0,.6);
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  left:0;
  top:0;
  padding:0 76rpx;
}
.commontoast .wrp{
  background:#fff;
  width:100%;
  box-sizing: border-box;
  border-radius:20rpx;
  text-align: center;
  padding:40rpx 30rpx;
  position: relative;
}
.commontoast .title{
  font-size:36rpx;
  font-weight:bold;
}
.commontoast .content{
  font-size:28rpx;
  padding:50rpx 0;
}
.commontoast .btns{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.commontoast .btn{
  display: inline-block;
  border-radius:38rpx;
  padding:20rpx 70rpx;
  margin:0 16rpx;
  font-size: 30rpx;
}
.commontoast .btn.btn-default{
  color:#fff;
}
.commontoast .closeBtn{
  position: absolute;
  bottom:-100rpx;
  left:50%;
  margin-left:-30rpx;
}

.commonimgcont{
  width:100%;
}
.commonimgcont.type2 image-loader{
  position: relative;
}
.commonimgcont.type1::after{
  content:'';
  clear:both;
  display: block;
  height: 0;
  visibility: hidden;
}
.commonimgcont.type1 .imgwrp{
  float:left;
  width:33.3333333333333%;
  box-sizing: border-box;
  padding:0 10rpx 10rpx 0;
}
.commonimgcont.type2 .imgwrp{
  float:none;
  width:100%;
}
.commonimgcont.type1 .imgwrp .subwrp{
  width:100%;
  padding-bottom:100%;
  position: relative;
}
.commonimgcont.type1 .imgwrp .subwrp image{
  left:0;
  position: absolute;
  top:0;
  border-radius: 6rpx;
  height:100%;
}
.commonimgcont .imgwrp .subwrp image{
  width:100%;
}
.commonimgcont.type3 .imgwrp:nth-of-type(2n-1){
  clear:both;
}

.nodata{
  width:100%;
  height:100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.nodata .title{
  font-size:28rpx;
  text-align: center;
  margin-top:20rpx;
  color:#ACBDD4;
}
.btn-disabled{
  background:#ddd!important;
}