/* pages/mall/goodsdetail/goodsdetail.wxss */
@import "wxParse/wxParse.wxss";
page{
  background:#f2f2f2;
}
.container{
  padding-bottom: 162rpx;
}
.goodsimage swiper{
  width:100%;
  height:750rpx;
}
.goodsimage image{
  width:100%;
  height:100%;
}
.goodsimage .info{
  width:100%;
  box-sizing: border-box;
  padding:0 30rpx;
  position: relative;
  font-size:24rpx;
  color:#666;
  margin-top:0;
}
.goodsimage .info .wrp{
  background:#fff;
  width:100%;
  min-height:142rpx;
  border:1rpx solid rgba(238,238,238,1);
  border-radius:10rpx;
  top:-30rpx;
  box-sizing: border-box;
  padding:30rpx;
  position:relative;
}
.exchangeoption{
  position: absolute;
  top:30rpx;
  right:30rpx;
  color:#666;
}
.goodsimage .info .name{
  font-size:28rpx;
  font-weight:bold;
}
.goodsimage .info .subinfo{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  margin-top:10rpx;
}
.goodsimage .info .subinfo .dk{
  display: flex;
  flex-direction: row;
  align-items: center;
  height:30rpx;
}
.goodsimage .info .subinfo .dk image{
  width:26rpx;
  height:30rpx;
  margin-right:10rpx;
}
.goodsimage .info .subinfo .dk text{
  color:#FF7E00;
  font-size:24rpx;
  display: inline-block;
  height:30rpx;
  line-height: 34rpx;
}
.goodsimage .info .subinfo .dk .price{
  margin-left:20rpx;
}
.goodsimage .info .subinfo .dk .price text{
  color:#666;
  font-size:24rpx;
}
.goodsimage .info .subinfo .r{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  color:#666;
}
.goodsimage .info .subinfo .baoyou{
  margin-right:30rpx;
}
.record{
  margin-top:30rpx;
  padding:0 30rpx;
  width:100%;
  box-sizing: border-box;
  position: relative;
}
.record .wrp{
  height:108rpx;
  line-height: 108rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-start;
}
.record .wrp,.notice .wrp{
  background:#fff;
  border:1rpx solid rgba(238,238,238,1);
  border-radius:10rpx;
  padding:0 30rpx;
  box-sizing: border-box;
  position: relative;
}
.record .num{
  font-size:20rpx;
  color:#999;
}
.recordlist image-loader{
  display: inline-block;
  width:65rpx;
  height:65rpx;
}
.recordlist .imgwrp{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  border-radius: 50%;
  overflow: hidden;
  margin-right:12rpx;
}
.recordlist image{
  width:100%;
  height:100%;
  display: block;
}
.recordlist{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-start;
  flex-grow: 1;
}
.comtitle{
  font-size:28rpx;
  font-weight:bold;
}
.more{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  height:100%;
  margin-left:20rpx;
}
.more image{
  width:15rpx;
  height:30rpx;
}
.notice{
  padding:0 30rpx;
  width:100%;
  box-sizing: border-box;
}
.notice .wrp{
  margin-top:20rpx;
  padding:30rpx 50rpx;
}
.notice .comtitle{
  text-align: center;
  margin-bottom: 16rpx;
}
.notice .content{
  font-size:24rpx;
  line-height:36rpx;
  color:#666;
}
.comment{
  background:#fff;
  margin-top:20rpx;
  padding:30rpx 30rpx 0 30rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid #eee;
}
.comment .comtitle{
  text-align: center;
  margin-bottom: 16rpx;
}
.comment .wrp{
  position: relative;
  box-sizing: border-box;
  padding:0 75rpx 0 105rpx;
}
.comment .wrp .photo{
  position: absolute;
  left:0;
  top:0;
}
.comment .wrp .photo image{
  width:90rpx;
  height:90rpx;
  border-radius: 50%;
}
.comment .wrp .content .name{
  font-size:28rpx;
  font-weight:bold;
}
.comment .wrp .content .p{
  font-size:28rpx;
  line-height:40rpx;
  color:#666;
  margin:12rpx 0 20rpx;
}
.comment .wrp .content .pics{
  width:100%;
  box-sizing: border-box;
  font-size: 0;
  position: relative;
}
.comment .wrp .content .pics image{
  border-radius: 10rpx;
  width:100%;
  height:100%;
  position: absolute;
  left:0;
  top:0;
}
.comment .wrp .content .pics .l{
  float:left;
  width:330rpx;
  box-sizing: border-box;
  margin-right:20rpx;
}
.comment .wrp .content .pics .r{
  float:left;
  width:155rpx;
  box-sizing: border-box;
}
.comment .wrp .content .pics .imgwrp{
  width:100%;
  padding-bottom: 100%;
  position: relative;
}
.comment .wrp .content .pics .r .iwrp{
  width:100%;
  position: relative;
  padding-bottom: 100%;
  box-sizing: border-box;
}
.comment .wrp .content .pics .r .iwrp:first-child{
  margin-bottom: 20rpx;
}
/* .comment .wrp .content .pics .l image{
  width:330rpx;
  height:330rpx;
}
.comment .wrp .content .pics .r image{
  width:155rpx;
  height:155rpx;
} */
.comment .wrp .content .pics .r{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  height:100%;
}
.comment .wrp .content .pics .num{
  position: absolute;
  right:30rpx;
  bottom:30rpx;
  color:#fff;
  background:rgba(0,0,0,0.29);
  border-radius:13rpx;
  font-size:20rpx;
  width:48rpx;
  text-align: center;
}
.viewMore{
  width:261rpx;
  height:70rpx;
  border-radius:35rpx;
  font-size:24rpx;
  color:rgba(229,130,6,1);
  margin:30rpx auto;
  text-align: center;
  line-height: 70rpx;
}
.detailinfo{
  background:#fff;
}
.detailinfo .comtitle{
  text-align: center;
  padding:26rpx 0;
}
.btm{
  position: fixed;
  left:0;
  bottom: 0;
  width:100%;
  height:132rpx;
  background: #fff;
  box-sizing: border-box;
  padding:0 30rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-start;
  color:#666;
  border-top:1rpx solid #eee;
}
.btm image{
  width:50rpx;
  height:50rpx;
}
.btm .btn{
  width:510rpx;
  height:86rpx;
  line-height:86rpx;
  background:linear-gradient(0deg,rgba(244,181,68,1) 0%,rgba(242,162,65,1) 100%);
  border-radius:43rpx;
  font-size:40rpx;
  font-weight:bold;
  color:rgba(254,254,254,1);
  text-align: center;
  display: inline-block;
  flex-grow: 1;
}
.btm .kefu{
  margin:0 40rpx;
}
.goodsintro image{
  width:100%;
}
.noshaidan{
  text-align: center;
  padding-bottom: 50rpx;
}
.goodpreview{
  position: fixed;
  width:100%;
  height:100%;
  left:0;
  top:0;
  display: none;
}
.goodpreview .mask{
  width:100%;
  height:100%;
  background:rgba(0,0,0,.27);
  position: absolute;
  left:0;
  top:0;
  z-index: 9999;
}
.goodpreview.show{
  display: block;
}
.goodpreview .wrp{
  padding:30rpx 30rpx 0 50rpx;
  position:absolute;
  left:0;
  bottom:0;
  background:#fff;
  width:100%;
  box-sizing: border-box;
  height:0;
  z-index: 10000;
}
.goodpreview.show .wrp{
  height:454rpx;
}
.goodpreview .t{
  border-bottom: 1rpx solid #eee;
  box-sizing: border-box;
  padding-left:210rpx;
  position: relative;
  padding-bottom: 40rpx;
}
.goodpreview .info{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  color:#999;
  font-size:24rpx;
  margin-top:20rpx;
}
.goodpreview .t>image-loader{
  width:180rpx;
  height:180rpx;
  position: absolute;
  left:0;
  top:-93rpx;
}
.goodpreview .t>image-loader image{
  width:100%;
  height:100%;
}
.buynum{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  padding:50rpx 0;
}
.buynum .title{
  font-size:32rpx;
  font-weight:bold;
}
.exchangeBtnTo{
  width:580rpx;
  height:86rpx;
  border-radius:43rpx;
  font-size:40rpx;
  font-weight:bold;
  color:#fff;
  text-align: center;
  line-height: 86rpx;
  margin:0 auto;
}
.record.ivtrecord{
  margin-top:30rpx;
}
.price{
  text-decoration:line-through;
  color:#666;
  flex-grow: 1;
  padding-left:20rpx;
}
.inviteMore{
  width:64rpx;
  height:64rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  background:#EEEEEE;
  border-radius: 50%;
}
.inviteMore image{
  width:36rpx;
  height:36rpx;
}
.standard{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-around;
  background:#fff;
  color:#999;
  font-size:20rpx;
  height: 112rpx;
  margin:0 30rpx;
  margin-top:-10rpx;
}
.standard .itm{
  text-align: center;
}
.standard .itm image{
  width:44rpx;
  height:44rpx;
}