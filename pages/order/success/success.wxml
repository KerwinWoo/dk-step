<!--pages/order/success/success.wxml-->
<scroll-view class="container hastop">
  <dk-nav title="兑换成功" hasBackBtn="{{true}}" isFixed="{{true}}" bind:backTo="backTo"></dk-nav>
  <view class="title">
    <image src="https://dkstep.oss-cn-beijing.aliyuncs.com/dkstep-img/status_success.png"></image>
    <text>兑换成功</text>
  </view>
  <view class="bg">
    <image src="https://dkstep.oss-cn-beijing.aliyuncs.com/dkstep-img/gift.png"></image>
  </view>
  <view class="tip">
    <view>订单已提交至后台处理</view>
    <view>消息分享给好友，可提高邀请成功率哦！</view>
  </view>
  <button class="sharebtn btn-default" open-type="share">快去分享给好友</button>
</scroll-view>
